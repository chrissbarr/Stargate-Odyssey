        <arguments>
          <argument>
            <kind>1</kind>
            <string>/*GAMECAVE EFFECTS ENGINE 3.X
Read the GCE3X documentation before using this code!

Object Name - objOverheadsnow
Engine - Overhead-Snow Engine 
Purpose - The controller declares particle types and bursts the snow.
*/

global.sky_type = -2; //Turn off sky drawing.

//System/Emitters
system = part_system_create(); //Main System

emitter_l = part_emitter_create(system); //Emitter for left side.
emitter_r = part_emitter_create(system); //Emitter for right side.

emitter_t = part_emitter_create(system); //Emitter for top side.
emitter_b = part_emitter_create(system); //Emitter for bottom side.

//Set initial regions.
part_emitter_region(system,emitter_l,view_xview[0]-10,view_xview[0],view_yview[0],view_yview[0]+view_hview[0],ps_shape_rectangle,ps_distr_linear);
part_emitter_region(system,emitter_r,view_xview[0]+view_wview[0],view_xview[0]+view_wview[0]+10,view_yview[0],view_yview[0]+view_hview[0],ps_shape_rectangle,ps_distr_linear);

part_emitter_region(system,emitter_t,view_xview[0]+10,view_xview[0]+view_wview[0],view_yview[0]-10,view_yview[0],ps_shape_rectangle,ps_distr_linear);
part_emitter_region(system,emitter_b,view_xview[0],view_xview[0]+view_wview[0],view_yview[0]+view_hview[0],view_yview[0]+view_hview[0]+10,ps_shape_rectangle,ps_distr_linear);

//SNOW Particle
snow = part_type_create();
part_type_color_mix(snow,c_white,c_ltgray);
part_type_direction(snow,0,360,0,0);
part_type_shape(snow,pt_shape_disk);
part_type_size(snow,0.1,0.1,-0.0002,0);
part_type_speed(snow,1,1,0,0);
part_type_life(snow,300,500);
part_type_alpha2(snow,1,0);</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
