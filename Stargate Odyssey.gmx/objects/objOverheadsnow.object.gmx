        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Burst Left-Side Snow
part_type_direction(snow,0,360,-7+random(14),0);
part_emitter_burst(system,emitter_l,snow,1);

//Burst Right-Side Snow
part_type_direction(snow,0,360,-7+random(14),0);
part_emitter_burst(system,emitter_r,snow,1);

//Burst Top-Side Snow
part_type_direction(snow,0,360,-7+random(14),0);
part_emitter_burst(system,emitter_t,snow,1);

//Burst Bottom-Side Snow
part_type_direction(snow,0,360,-7+random(14),0);
part_emitter_burst(system,emitter_b,snow,1);

//Control View
if keyboard_check(vk_up){view_yview[0]=max(view_yview[0]-3,0)}
if keyboard_check(vk_down){view_yview[0]=min(view_yview[0]+3,room_height)}
if keyboard_check(vk_left){view_xview[0]=max(view_xview[0]-3,0)}
if keyboard_check(vk_right){view_xview[0]=min(view_xview[0]+3,room_width)}

//Update the position to create snow particles.
part_emitter_region(system,emitter_l,view_xview[0]-10,view_xview[0],view_yview[0],view_yview[0]+view_hview[0],ps_shape_rectangle,ps_distr_linear);
part_emitter_region(system,emitter_r,view_xview[0]+view_wview[0],view_xview[0]+view_wview[0]+10,view_yview[0],view_yview[0]+view_hview[0],ps_shape_rectangle,ps_distr_linear);

part_emitter_region(system,emitter_t,view_xview[0]+10,view_xview[0]+view_wview[0],view_yview[0]-10,view_yview[0],ps_shape_rectangle,ps_distr_linear);
part_emitter_region(system,emitter_b,view_xview[0],view_xview[0]+view_wview[0],view_yview[0]+view_hview[0],view_yview[0]+view_hview[0]+10,ps_shape_rectangle,ps_distr_linear);</string>
          </argument>
        </arguments>
      </action>
