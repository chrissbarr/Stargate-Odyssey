global.sky_type = 0; //Set this engines sky.
cloud_number = 5; //How many clouds to have on the screen.
cloud_intensity = 20; //How many cloud particles to have in each cloud.

//Particle Systems
clouds = part_system_create() //The system for the louds

//Cloud parts
cloud = part_type_create()
part_type_life(cloud,3000,3000)
part_type_shape(cloud,pt_shape_cloud)
part_type_size(cloud,1,2,0,0.02)

//Cloud Emitters
var xx,yy;

for(i=0;i&lt;cloud_number;i+=1)
{
    xx = random(room_width);
    yy = random(room_height);
    emitter[i] = part_emitter_create(clouds);
    part_emitter_region(clouds,emitter[i],xx-60-random(100),xx+60+random(100),yy-40-random(40),yy+40+random(40),ps_shape_ellipse,ps_distr_gaussian);
    part_emitter_burst(clouds,emitter[i],cloud,cloud_intensity-random(cloud_intensity/2)+random(cloud_intensity));
}
</string>
