            <kind>1</kind>
            <string>/*GAMECAVE EFFECTS ENGINE 3.X
Read the GCE3X documentation before using this code!

Object Name - objClouds
Engine - Cloud Engine
Purpose - The master object for clouds; sets paramaters and handles the particles.
*/

global.sky_type = 0; //Set this engines sky.

//Particle Systems
clouds_infront = part_system_create() //The system for the fore-clouds
part_system_depth(clouds_infront,5) //Set the depth for fore-clouds
clouds_behind = part_system_create() //Create the back-clouds
part_system_depth(clouds_behind,10) //Set the depth for back-clouds.

//Cloud (In Front)
cloud0 = part_type_create()
part_type_alpha2(cloud0,0.3,0)
part_type_color2(cloud0,c_white,c_ltgray)
part_type_direction(cloud0,180,180,0,0)
part_type_life(cloud0,700,750)
part_type_speed(cloud0,1,2,0,0.5)
part_type_shape(cloud0,pt_shape_cloud)
part_type_size(cloud0,1,2,0,0.02)

//Cloud (Behind)
cloud1 = part_type_create()
part_type_alpha2(cloud1,0.8,0)
part_type_color2(cloud1,c_white,c_ltgray)
part_type_direction(cloud1,180,180,0,0)
part_type_life(cloud1,700,750)
part_type_speed(cloud1,0.5,1,0,0.5)
part_type_shape(cloud1,pt_shape_cloud)
part_type_size(cloud1,0.6,1.5,0,0.02)

//Emitter
emitter1 = part_emitter_create(clouds_infront)
part_emitter_region(clouds_infront,emitter1,room_width+20,room_width+40,0,room_height,ps_shape_rectangle,ps_distr_linear)
part_emitter_stream(clouds_infront,emitter1,cloud0,1) //Starts streaming the back-clouds.

emitter2 = part_emitter_create(clouds_behind)
part_emitter_region(clouds_behind,emitter2,room_width+20,room_width+40,0,room_height,ps_shape_rectangle,ps_distr_linear)
part_emitter_stream(clouds_behind,emitter2,cloud1,1) //Starts streaming the fore-clouds.

repeat(400) {part_system_update(clouds_infront); part_system_update(clouds_behind);} //"Fast forward" the clouds movement so it's in the middle of the screen when first shown.
</string>
